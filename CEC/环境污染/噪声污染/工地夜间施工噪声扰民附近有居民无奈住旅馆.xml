<?xml version="1.0" encoding="UTF-8"?>

<Body> 
	<Title>工地夜间施工噪声扰民附近有居民无奈住旅馆</Title>  
	<ReportTime type="absTime">2014-01-01 07:00　</ReportTime>  
	<Content> 
		<Paragraph> 
			<Sentence> 
				<Event eid="e1"> 
					<Time tid="t1" type="absTime">2013年12月26日凌晨2时</Time>， 
					<Participant sid="s1-s3">记者</Participant>从 
					<Location lid="l1" type="origin">北环路</Location>  
					<Denoter did="d1" type="movement">拐进</Denoter>  
					<Location lid="l1" type="destination">五邑锦绣豪庭前的马路</Location> 
				</Event>， 
				<Event eid="e2">沿路 
					<Denoter did="d2" type="movement">走到</Denoter>  
					<Location lid="l2" type="destination">蓬江区棠下镇新昌村工业区入口告示牌旁边</Location> 
				</Event>， 
				<Event eid="e3" type="thoughtevent"> 
					<Denoter did="d3" type="perception">看到</Denoter> 
				</Event>  
				<Event eid="e4"> 
					<Location lid="l4">一个楼盘工地</Location>内 
					<Denoter did="d4">灯火通明</Denoter> 
				</Event>， 
				<Event eid="e5">还有大量的 
					<Object sid="s5">泥水</Object>从 
					<Location lid="l5" type="origin">工地</Location>内 
					<Denoter did="d5" type="movement">流出</Denoter> 
				</Event>。
			</Sentence>  
			<Sentence> 
				<Event eid="e6" type="thoughtevent">一位 
					<Location lid="l6">五邑锦绣豪庭</Location>的 
					<Participant sid="s6">住户</Participant>  
					<Denoter did="d6" type="statement">告诉</Denoter>  
					<Participant oid="o6">记者</Participant> 
				</Event>， 
				<Event eid="e7"> 
					<Object sid="s7">施工</Object>从 
					<Time tid="t7" type="absTime">2013年12月25日傍晚</Time>一直 
					<Denoter did="d7">持续</Denoter>到 
					<Time tid="t7" type="relTime">26日凌晨</Time> 
				</Event>， 
				<Event eid="e8"> 
					<Object sid="s8">打桩声</Object>“ 
					<Denoter did="d8">让人烦躁</Denoter>”
				</Event>。
			</Sentence> 
		</Paragraph> 
	</Content>  
	<eRelation relType="Follow" bevent_eid="e1" aevent_eid="e2"/>  
	<eRelation relType="Follow" bevent_eid="e2" aevent_eid="e3"/>  
