	<Content>　　 
		<Paragraph> 
			<Sentence> 
				<Event type="thoughtevent" eid="e1"> 
					<Participant sid="s1">台海网</Participant>  
					<Time tid="t1" type="relTime">9月13日</Time>  
					<Denoter did="d1" type="statement">讯</Denoter> 
				</Event> 
			</Sentence>  
			<Sentence> 
				<Event type="thoughtevent" eid="e2">据 
					<Location lid="l2">台湾</Location>  
					<Participant sid="s2">TVBS网站</Participant>  
					<Denoter did="d2" type="statement">报道</Denoter> 
				</Event>， 
				<Event eid="e3"> 
					<Location lid="l3">台湾桃园杨梅市</Location>发生 
					<Denoter did="d3" type="emergency">氨气外泄</Denoter>事件
