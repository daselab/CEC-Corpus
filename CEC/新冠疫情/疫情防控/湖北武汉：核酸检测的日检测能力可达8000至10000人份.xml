    <eRelation relType="Follow" bevent_eid="e11" aevent_eid="e12"/>
    <eRelation relType="Follow" bevent_eid="e12" aevent_eid="e13"/>
    <eRelation relType="Composite" fevent_eid="e14" sevent_eids="e10-e13"/>
    <eRelation relType="Follow" bevent_eid="e15" aevent_eid="e16-e20"/>
    <eRelation relType="Accompany" accompanya_eid="e18" accompanyb_eid="e19"/>
</Body>