                </Event>
                <Event eid="e45">
                    <Location lid="l45">随州市</Location>
                    <Participant oid="o45" type="recipient">953例</Participant>、
                </Event>
                <Event eid="e46">
                    <Location lid="l46">荆州市</Location>
                    <Participant oid="o46" type="recipient">941例</Participant>、
                </Event>
                <Event eid="e47">
                    <Location lid="l47">襄阳市</Location>
                    <Participant oid="o47" type="recipient">907例</Participant>、
                </Event>
                <Event eid="e48">
                    <Location lid="l48">黄石市</Location>
                    <Participant oid="o48" type="recipient">703例</Participant>、
                </Event>
                <Event eid="e49">
                    <Location lid="l49">宜昌市</Location>
                    <Participant oid="o49" type="recipient">633例</Participant>、
                </Event>
                <Event eid="e50">
                    <Location lid="l50">荆门市</Location>
                    <Participant oid="o50" type="recipient">588例</Participant>、
                </Event>
                <Event eid="e51">
                    <Location lid="l51">鄂州市</Location>
                    <Participant oid="o51" type="recipient">569例</Participant>、
                </Event>
                <Event eid="e52">
                    <Location lid="l52">咸宁市</Location>
                    <Participant oid="o52" type="recipient">476例</Participant>、
                </Event>
                <Event eid="e53">
                    <Location lid="l53">十堰市</Location>
