    <eRelation relType="Accompany" accompanya_eid="e17" accompanyb_eid="e18"/>
    <eRelation relType="Follow" bevent_eid="e20" aevent_eids="e21-e24"/>
    <eRelation relType="Follow" bevent_eid="e23" aevent_eid="e24"/>
    <eRelation relType="Accompany" accompanya_eid="e25" accompanyb_eid="e26"/>
</Body>